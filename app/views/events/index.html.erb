<div class='float-box col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2'>
	<h1>
		<%= link_to current_client.name, client_path(current_client) %>
  	<span class="glyphicon" style=" color: <%= current_client.glyph_color %>;	"><%= raw current_client.glyph_id %></span>
  </h1>
  <h2 class='highlight'><%= link_to current_project.name, project_tasks_path(current_project) %></h2>
	<h2 class='highlight'><%= link_to current_task.name, project_task_path(current_project, current_task) %></h2>
	<div class='item-box'> 
    <% @events.each do |event| %>
			<h2><%= link_to event.name, task_event_path(current_task, event) %></h2>
    <% end %>
    <h3>
      <%= link_to '+ Add New Event', project_task_path(current_project, current_task), class: 'add-new' %>
    </h3>
  </div>
  <%= render 'shared/back' %>
</div>