<div class='float-box col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2'>
	<h1><%= link_to current_project.name, client_project_path(current_client, current_project) %></h1>
	<h3><%= link_to current_client.name, client_projects_path(current_client) %> </h3>
	<% @tasks.each do |task| %>
		<div class='item-box'>	
			<h2><%= link_to task.name, project_task_path(current_project, task) %>
		</div>
	<% end %>	
	<%= link_to 'Back', :back, :class => 'btn btn-primary' %>
	<%= link_to 'New Task', new_project_task_path(current_project), :class => 'btn btn-primary' %>
</div>