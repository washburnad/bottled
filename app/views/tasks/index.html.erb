<div class='float-box col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2'>
	<h1 class='highlight'><%= link_to current_client.name, client_path(current_client) %></h1>
	<h2 class='highlight'><%= link_to current_project.name, project_tasks_path(current_project) %></h2>
	<% @tasks.each do |task| %>
		<div class='item-box'>	
			<h3><%= link_to task.name, project_task_path(current_project, task) %></h3
		</div>
	<% end %>	
	<%= link_to 'Back', :back, :class => 'btn btn-primary' %>
	<%= link_to 'New Task', new_project_task_path(current_project), :class => 'btn btn-primary' %>
</div>