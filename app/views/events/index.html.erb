<div class='float-box col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2'>
	<h1><%= link_to current_task.name, project_task_path(current_project,current_task) %><%= link_to current_project.name, client_project_path(current_client, current_project) %><span class='h1-small'> : <%= link_to current_client.name, client_projects_path(current_client) %> </span></h1>
	<% @events.each do |event| %>
		<div class='item-box'>	
			<h2><%= link_to event.name, task_event_path(current_task, event) %>
		</div>
	<% end %>	
	<%= link_to 'Back', :back, :class => 'btn btn-primary' %>
</div>